{
  "name": "takenote",
  "version": "0.7.2",
  "description": "A web-based notes app for developers.",
  "author": "Tania Rascia",
  "license": "MIT",
  "private": false,
  "main": "src/server/index.ts",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "client": "npx cross-env NODE_ENV=development webpack serve --config config/webpack.dev.js",
    "build": "npx cross-env NODE_ENV=production webpack --config config/webpack.prod.js",
    "prod": "node -r ts-node/register/transpile-only src/server/index.ts",
    "start": "npm run client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/taniarascia/takenote"
  },
